<html>
    <head>
            <style>
                    ul {
                      list-style-type: none;
                      margin: 0;
                      padding: 0;
                      overflow: hidden;
                      background-color: #333;
                    }
                    
                    li {
                      float: left;
                    }
                    
                    li a {
                      display: block;
                      color: white;
                      text-align: center;
                      padding: 14px 16px;
                      text-decoration: none;
                    }
                    
                    li a:hover {
                      background-color: #111;
                    }
                    </style>
    </head>
    <body>
            <ul>
                    <li><a class="active" href="#home">Home</a></li>
                    <li><a href="#news">News</a></li>
                    <li><a href="#contact">Contact</a></li>
                    <li><a href="#about">About</a></li>
                    <li><a href="/logout">logout</a></li>
                  </ul>                  
        <h1>Get Data</h1>
        <table border="1">
            <tr>
                <td>id</td>
                <td>Name</td>
                <td>lastName</td>
                <td>midelname</td>
                <td>birthday</td>
                <td>Address1</td>
                <td>zipcode</td>
                <td>city</td>
                <td>Country</td>
                <td>code</td>
                <td>Mobile_Number</td>
                <td>Gender</td>
                <td>Hobby</td>
                <td>Email</td>
                <td>Password</td>
                <td>image</td>
                <td>message</td>
                <td>dateofjoin</td>
                <td>Interest</td>
                <td>Blood_Group</td>
                <td>Branch</td>
                <td>Roal</td>
            </tr>
            <% for(var i=0; i < data.length; i++) {%>
                <tr>
                    <td><%= data[i]._id %></td>
                    <td><%= data[i].Name %></td>
                    <td><%= data[i].lastName %></td>
                    <td><%= data[i].midelname %></td>
                    <td><%= data[i].birthday %></td>
                    <td><%= data[i].Address1 %></td>
                    <td><%= data[i].zipcode %></td>
                    <td><%= data[i].city %></td>
                    <td><%= data[i].Country %></td>
                    <td><%= data[i].code %></td>
                    <td><%= data[i].Mobile_Number %></td>
                    <td><%= data[i].Gender %></td>
                    <td><%= data[i].Hobby %></td>
                    <td><%= data[i].Email %></td>
                    <td><%= data[i].Password %></td>
                    <td><%= data[i].image %></td>
                    <td><%= data[i].message %></td>
                    <td><%= data[i].dateofjoin %></td>
                    <td><%= data[i].Interest %></td>
                    <td><%= data[i].Blood_Group %></td>
                    <td><%= data[i].Branch %></td>
                    <td><%= data[i].roal %></td>
                    <td><input type="button" value="Edit/Update" onclick="updatedata(data[i]._id)"></td>
                    <td><input type="button" value="Delete"></td>
                </tr>
             <% } %>
            </table>
            <script>
                    function updatedata(data){
                        console.log(data);
                    }
                </script>
    </body>
</html>